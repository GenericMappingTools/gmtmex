// Installation directory of MATLAB
MATLABROOT = $(LOCAL_APPS_DIR)/MATLAB_R2012b.app

// Build Options
GCC_VERSION = com.apple.compilers.llvmgcc42

// Deployment
MACOSX_DEPLOYMENT_TARGET = 10.7
